<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="common">
      <section name="logging" type="Common.Logging.ConfigurationSectionHandler, Common.Logging" requirePermission="false" />
    </sectionGroup>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <section name="rhino.esb" type="Rhino.ServiceBus.Config.BusConfigurationSection, Rhino.ServiceBus" />
  </configSections>

  <appSettings>
    
    <add key="ThreePublicKeyFolder" value="C:\Users\smarkey\Documents\GitHub\LayeredArchitecture\Miscellaneous\EncryptionKeys\ThreePublic" />
    <add key="ThreePrivateKeyFolder" value="C:\Users\smarkey\Documents\GitHub\LayeredArchitecture\Miscellaneous\EncryptionKeys\ThreePrivate" />
    <add key="InternalPublicKeyFolder" value="C:\Users\smarkey\Documents\GitHub\LayeredArchitecture\Miscellaneous\EncryptionKeys\InternalPublic" />
    <add key="InternalPrivateKeyFolder" value="C:\Users\smarkey\Documents\GitHub\LayeredArchitecture\Miscellaneous\EncryptionKeys\InternalPrivate" />
    <add key="RsaKeyFolder" value="C:\Users\smarkey\Documents\GitHub\LayeredArchitecture\Miscellaneous\EncryptionKeys" />
  </appSettings>

  <rhino.esb>
    <bus transactional="true" queueIsolationLevel="ReadCommitted" threadCount="5" numberOfRetries="3" endpoint="msmq://localhost/taskrunner" />
    <messages>
      <add name="TaskRunner.Common.Messages" endpoint="msmq://localhost/taskrunner" />
    </messages>
  </rhino.esb>
